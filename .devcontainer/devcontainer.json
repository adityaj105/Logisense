{
  "name": "LogiSense Dev",
  "dockerFile": "Dockerfile",
  "context": "..",
  "extensions": [
    "ms-python.python",
    "ms-python.vscode-pylance",
    "ms-azuretools.vscode-docker",
    "eamodio.gitlens",
    "streetsidesoftware.code-spell-checker"
  ],
  "forwardPorts": [
    8501,
    8000
  ],
  "postCreateCommand": "pip install -r /workspace/requirements.txt",
  "remoteUser": "vscode"
}